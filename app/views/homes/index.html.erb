<p id="notice"><%= notice %></p>

<h1>Homes</h1>




<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Postalcode</th>
      <th>Country</th>
      <th>Province</th>
      <th>Location</th>
      <th>Reference</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homes.each do |home| %>
      <tr>
        <td><%= home.account.first_name %></td>
        <td><%= home.address %></td>
        <td><%= home.postalcode %></td>
        <td><%= home.country %></td>
        <td><%= home.province %></td>
        <td><%= home.location %></td>
        <td><%= home.reference %></td>
        <td><%= link_to 'Show', home %></td>
        <td><%= link_to 'Edit', edit_home_path(home) %></td>
        <td><%= link_to 'Destroy', home, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home', new_home_path %>
